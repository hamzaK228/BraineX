{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/frontend/public/assets/$1"
    },
    {
      "src": "/sw.js",
      "dest": "/frontend/public/sw.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/frontend/manifest.json"
    },
    {
      "src": "/favicon.ico",
      "dest": "/frontend/public/favicon.ico"
    },
    {
      "src": "/",
      "dest": "/frontend/pages/main.html"
    },
    {
      "src": "/fields",
      "dest": "/frontend/pages/fields.html"
    },
    {
      "src": "/universities",
      "dest": "/frontend/pages/universities.html"
    },
    {
      "src": "/programs",
      "dest": "/frontend/pages/programs.html"
    },
    {
      "src": "/scholarships",
      "dest": "/frontend/pages/scholarships.html"
    },
    {
      "src": "/projects",
      "dest": "/frontend/pages/projects.html"
    },
    {
      "src": "/roadmaps",
      "dest": "/frontend/pages/roadmaps.html"
    },
    {
      "src": "/mentors",
      "dest": "/frontend/pages/mentors.html"
    },
    {
      "src": "/events",
      "dest": "/frontend/pages/events.html"
    },
    {
      "src": "/about",
      "dest": "/frontend/pages/about.html"
    },
    {
      "src": "/admin",
      "dest": "/frontend/pages/admin.html"
    },
    {
      "src": "/notion",
      "dest": "/frontend/pages/notion.html"
    }
  ]
}