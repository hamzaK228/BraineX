{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/frontend/assets/$1"
    },
    {
      "src": "/sw.js",
      "dest": "/frontend/sw.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/frontend/manifest.json"
    },
    {
      "src": "/robots.txt",
      "dest": "/frontend/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/frontend/sitemap.xml"
    },
    {
      "src": "/favicon.ico",
      "dest": "/frontend/favicon.ico"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/fields",
      "dest": "/frontend/fields.html"
    },
    {
      "src": "/universities",
      "dest": "/frontend/universities.html"
    },
    {
      "src": "/programs",
      "dest": "/frontend/programs.html"
    },
    {
      "src": "/scholarships",
      "dest": "/frontend/scholarships.html"
    },
    {
      "src": "/projects",
      "dest": "/frontend/projects.html"
    },
    {
      "src": "/roadmaps",
      "dest": "/frontend/roadmaps.html"
    },
    {
      "src": "/mentors",
      "dest": "/frontend/mentors.html"
    },
    {
      "src": "/events",
      "dest": "/frontend/events.html"
    },
    {
      "src": "/about",
      "dest": "/frontend/about.html"
    },
    {
      "src": "/admin",
      "dest": "/frontend/admin.html"
    },
    {
      "src": "/notion",
      "dest": "/frontend/notion.html"
    },
    {
      "src": "/goals",
      "dest": "/frontend/notion.html"
    },
    {
      "src": "/gym",
      "dest": "/frontend/gym.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/404.html",
      "status": 404
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}